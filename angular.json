{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-mat-datepicker-simple-formatting": {
      "projectType": "library",
      "root": "projects/ngx-mat-datepicker-simple-formatting",
      "sourceRoot": "projects/ngx-mat-datepicker-simple-formatting/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-mat-datepicker-simple-formatting/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-mat-datepicker-simple-formatting/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-mat-datepicker-simple-formatting/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-mat-datepicker-simple-formatting/src/test.ts",
            "tsConfig": "projects/ngx-mat-datepicker-simple-formatting/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-mat-datepicker-simple-formatting/karma.conf.js"
          }
        }
      }
    },
    "demo-site": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/demo-site",
      "sourceRoot": "projects/demo-site/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo-site",
            "index": "projects/demo-site/src/index.html",
            "main": "projects/demo-site/src/main.ts",
            "polyfills": "projects/demo-site/src/polyfills.ts",
            "tsConfig": "projects/demo-site/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/demo-site/src/favicon.ico",
              "projects/demo-site/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/demo-site/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/demo-site/src/environments/environment.ts",
                  "with": "projects/demo-site/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "demo-site:build:production"
            },
            "development": {
              "browserTarget": "demo-site:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo-site:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/demo-site/src/test.ts",
            "polyfills": "projects/demo-site/src/polyfills.ts",
            "tsConfig": "projects/demo-site/tsconfig.spec.json",
            "karmaConfig": "projects/demo-site/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/demo-site/src/favicon.ico",
              "projects/demo-site/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/demo-site/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "ngx-mask-date": {
      "projectType": "library",
      "root": "projects/ngx-mask-date",
      "sourceRoot": "projects/ngx-mask-date/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-mask-date/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-mask-date/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-mask-date/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-mask-date/src/test.ts",
            "tsConfig": "projects/ngx-mask-date/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-mask-date/karma.conf.js"
          }
        }
      }
    },
    "ngx-formcontrol-companion": {
      "projectType": "library",
      "root": "projects/ngx-formcontrol-companion",
      "sourceRoot": "projects/ngx-formcontrol-companion/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-formcontrol-companion/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-formcontrol-companion/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-formcontrol-companion/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-formcontrol-companion/src/test.ts",
            "tsConfig": "projects/ngx-formcontrol-companion/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-formcontrol-companion/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "ngx-mat-datepicker-simple-formatting"
}
